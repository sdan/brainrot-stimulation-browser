{
  "manifest_version": 3,
  "name": "ChatGPT Video Overlays",
  "version": "1.0",
  "description": "Adds video overlays to the sides of ChatGPT interface",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
